{
  "manifest_version": 2,

  "name": "iSaver",
  "description": "A simple extension to remind you to give you eyes a break!.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "eye.png",
    "default_popup": "popup.html",
    "default_title": "Rest your eyes!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
